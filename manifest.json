{
  "manifest_version": 3,
  "name": "Local Storage Explorer",
  "short_name": "LocalStorageX",
  "version": "2.0.0",
  "minimum_chrome_version": "100",
  "description": "Developer tool for exploring localStorage and sessionStorage which parses JSON values",
  "author": "Deliaz",
  "offline_enabled": true,
  "icons": {
    "38": "imgs/38.png",
    "48": "imgs/48.png",
    "128": "imgs/128.png"
  },
  "devtools_page": "loader.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
